{
  "order": 10,
  "index_patterns": [ "logs-*" ],
  "version": 2019090101,
  "settings": {
    "index": {
      "mapping": {
        "ignore_malformed": true,
        "total_fields.limit": "10000",
        "coerce": true
      }
    },
    "refresh_interval": "10s",
    "number_of_replicas": 0,
    "number_of_shards": 2
  },
  "mappings": {
    "dynamic": "true",
    "dynamic_templates": [
      {
        "strings": {
          "match_mapping_type": "string",
          "mapping": {
            "ignore_above": 4096,
            "type": "text",
            "fields": {
              "text": {
                "type": "keyword"
              }
            }
          }
        }
      }
    ],
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "@version": {
        "type": "keyword"
      },
      "z_original_timestamp": {
        "type": "date"
      },
      "meta_log_tags": {
        "type": "keyword",
        "fields": {
          "text": {
            "type": "text"
          }
        }
      },
      "z_logstash_type": {
        "enabled": false
      },
      "event_original_message": {
        "type": "text",
        "norms": false
      },
      "event_etl_pipeline": {
        "type": "keyword"
      },
      "event_etl_processed_timestamp": {
        "type": "date"
      },
      "message": {
        "type": "alias",
        "path": "event_original_message"
      }
    }
  }
}
